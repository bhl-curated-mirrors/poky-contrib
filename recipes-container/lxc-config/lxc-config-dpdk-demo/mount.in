# lxc mount setting
lxc.mount.entry = /var/lib/lxc/dpdk-demo/system.conf etc/systemd/system.conf.d/10-environment.conf none bind,optional,create=file

lxc.mount.auto = cgroup:mixed proc:mixed sys:mixed
lxc.mount.entry = shm /dev/shm tmpfs defaults 0 0

lxc.mount.entry = /dev/hugepages dev/hugepages none bind,create=dir 0 0
lxc.mount.entry = /tmp/dpdk /tmp/dpdk none bind,,create=dir 0 0
